{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "typeRoots": ["node_modules/@types"],
    "types": ["node"],
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
		"baseUrl": "src",
    "paths": {
      "*": ["src/*"]
    },
  },
  "exclude": ["node_modules/aria-query"],
  "include": ["src"]
}